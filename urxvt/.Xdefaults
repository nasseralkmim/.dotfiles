! #include </home/nasser/nord.Xcolors>

URxvt*font: xft:Iosevka SS05:size=9:antialias=true
URxvt.boldFont: xft:DejaVu Sans Mono:pixelsize=9:weight=bold

URxvt*italicFont: 
URxvt*bolditalicFont: 

URxvt*letterSpace: -1
URxvt*lineSpace: -1

! █▓▒░ interface
URxvt*scrollBar: false
URxvt*transparent: false
URxvt*internalBorder: 10
URxvt*borderLess: true

! █▓▒░ extensions

URxvt.perl-ext: default,clipboard,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.underline: false
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.autocopy: true
URxvt.clipboard.autocopy: true
URxvt.clipboard.copycmd: xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.keysym.C-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

! █▓▒░ misc
URxvt.saveLines: 9999
URxvt.iso14755: false
!URxvt*altSendsEscape: true

! █▓▒░ high dpi mode
Xft.dpi: 190
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

twobwm.enable_compton: true

! Tango theme

URxvt.background: #1C1C1C
URxvt.foreground: #DEDEDE

! black
URxvt.color0  : #2E3436
URxvt.color8  : #555753
! red
URxvt.color1  : #CC0000
URxvt.color9  : #EF2929
! green
URxvt.color2  : #4E9A06
URxvt.color10 : #8AE234
! yellow
URxvt.color3  : #C4A000
URxvt.color11 : #FCE94F
! blue
URxvt.color4  : #3465A4
URxvt.color12 : #729FCF
! magenta
URxvt.color5  : #75507B
URxvt.color13 : #AD7FA8
! cyan
URxvt.color6  : #06989A
URxvt.color14 : #34E2E2
! white
URxvt.color7  : #D3D7CF
URxvt.color15 : #EEEEEC
